{
  "name": "@materialthing/jsx",
  "version": "4.0.0",
  "description": "JSX runtime for MaterialThing framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./jsx-runtime": {
      "import": "./dist/jsx-runtime.js",
      "types": "./dist/jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/jsx-dev-runtime.js",
      "types": "./dist/jsx-dev-runtime.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup index.ts jsx-runtime.ts jsx-dev-runtime.ts --format esm,cjs --dts",
    "dev": "npm run build -- --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@materialthing/core": "workspace:*"
  },
  "keywords": [
    "jsx",
    "runtime",
    "react-jsx"
  ],
  "author": "Oarabile Koore",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
